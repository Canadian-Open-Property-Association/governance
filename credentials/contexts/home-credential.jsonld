{
  "$id": "https://openpropertyassociation.ca/credentials/schemas/home-credential.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Home Credential",
  "description": "Home Credential",
  "type": "object",
  "properties": {
    "credentialSubject": {
      "type": "object",
      "properties": {
        "Unit Number": {
          "type": "string"
        },
        "Street Number": {
          "type": "string"
        },
        "Street Name": {
          "type": "string"
        },
        "Postal Code": {
          "type": "string"
        },
        "City": {
          "type": "string",
          "format": "date"
        },
        "Purchase Price": {
          "type": "string"
        },
        "Purchase Date": {
          "type": "string"
        },
        "Year Built": {
          "type": "string"
        },
        "Neighbourhood": {
          "type": "string"
        }
      }
    }
  }
}